<% include partials/header.ejs %>
    <div class="container">

        <h1>Videos</h1>
        <div>
          <% videos.forEach((video) => { %>
            <div>
              <a href="/videos/<%= video.id %>"><%= video.title %></a>
            </div>
          <% }) %>
        </div>

        <% if (message != '') { %>
            <p class="text-center text-danger"><%= message %></p>
        <% } %>
                
    </div>
</div>
</body>
</html>
